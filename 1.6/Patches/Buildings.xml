<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="Bookcase"]</xpath>
    <value>
      <li Class="HSK_Storage_Extensions.StorageDefExtension">
        <stowingProperties>
          <additionalTicksPerStoredStack>500</additionalTicksPerStoredStack>
          <additionalTicksPerStoredDef>500</additionalTicksPerStoredDef>
        </stowingProperties>
      </li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAddModExtension">
    <xpath>Defs/ThingDef[defName="Shelf"]</xpath>
    <value>
      <li Class="HSK_Storage_Extensions.StorageDefExtension">
        <stowingProperties>
          <additionalTicksPerStoredStack>500</additionalTicksPerStoredStack>
          <additionalTicksPerStoredDef>500</additionalTicksPerStoredDef>

          <quickToStoreItems>
            <categories>
              <li>Foods</li>
            </categories>
          </quickToStoreItems>
        </stowingProperties>
      </li>
    </value>
  </Operation>
</Patch>